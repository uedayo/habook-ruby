<table class="table">
    <thead>
        <tr>
            <td></td>
            <td>タイトル</td>
            <td>著者</td>
            <td>出版社</td>
            <td>ISBN</td>
            <td></td>
            <td>状態</td>
        </tr>
    </thead>
    <tbody>
        <tr id="book_<%= @book.id %>">
            <td><%= link_to image_tag(@book.medium_image, :width => "100"), @book.detail_page_url %></td>
            <td><%= link_to @book.title, book_path(@book) %></td>
            <td><%= @book.author %></td>
            <td><%= @book.manufacturer %></td>
            <td><%= @book.isbn %></td>
            <td><%= link_to "amazon", @book.detail_page_url%></td>
            <td><p class="text-success"><%= @book.user_id %></p></td>
        </tr>
    </tbody>
</table>